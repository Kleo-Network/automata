{
    "manifest_version": 3,
    "name": "Kleo Network",
    "version": "3.0",
    "description": "Kleo Network is a user consumption DATA DAO. Kleo ",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "js/index.html",
      "default_title": "Kleo Network: Extension"
    },
    "icons":{
      "128" : "logo/128.png",
      "64": "logo/64.png",
      "32": "logo/32.png",
      "16": "logo/16.png" 
    },
    "permissions": [
        "tabs",
        "history",
        "storage"
      ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["injectedScript.js"],
        "matches": ["http://localhost:5173/*","https://*.kleo.network/*"]
      }
    ]
  }
  