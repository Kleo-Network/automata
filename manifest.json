{
    "manifest_version": 3,
    "name": "Automata",
    "version": "3.8",
    "description": "Automata is a user consumption DATA DAO. Use your data to it's maximum potential.",
    "background": {
      "service_worker": "background.js"
    },
    "side_panel": {
      "default_path": "index.html",
      "default_title": "Automata: Side Panel"
    },
    "icons":{
      "128" : "logo/128.png",
      "64": "logo/64.png",
      "32": "logo/32.png",
      "16": "logo/16.png" 
    },
    "permissions": [
        "tabs",
        "history",
        "storage",
        "sidePanel"
      ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
  }
  